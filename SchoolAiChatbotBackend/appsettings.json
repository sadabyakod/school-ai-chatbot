{
  "Jwt": {
    "Key": "YOUR_SECRET_KEY_HERE",
    "Issuer": "SchoolAiChatbotBackend",
    "Audience": "SchoolAiChatbotUsers"
  },
  
  "DatabaseProvider": "SqlServer",
  
  "ConnectionStrings": {
    "DefaultConnection": "YOUR_SQL_SERVER_CONNECTION_STRING_HERE",
    "SqlDb": "YOUR_SQL_SERVER_CONNECTION_STRING_HERE"
  },
  
  "AzureWebJobsStorage": "YOUR_AZURE_STORAGE_CONNECTION_STRING_HERE",
  
  "AzureOpenAI": {
    "Endpoint": "https://YOUR_AZURE_OPENAI_ENDPOINT.openai.azure.com/",
    "ApiKey": "YOUR_AZURE_OPENAI_API_KEY_HERE",
    "ChatDeployment": "gpt-4",
    "EmbeddingDeployment": "text-embedding-3-small"
  },
  
  "OpenAI": {
    "ApiKey": "YOUR_OPENAI_API_KEY_HERE"
  },
  
  "USE_REAL_EMBEDDINGS": "true",

  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
